#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
  char *buffer;
  size_t len = 32;
  size_t characters;
  

  printf("Enter Input: ");
  characters = getline(&buffer, &len, stdin);
  printf("%zu characters were entered\n", characters);
  printf("You typed: '%s'\n", buffer);
  
  char *ptr = strtok_r(buffer, " ");
  while (ptr != NULL)
  {
    printf("'%s'\n", ptr);
    ptr = strtok(NULL, " ");
  }




  return 0;
}
